/root/apache-maven-3.6.3/bin/mvn -DskipTests=true clean package -Dmaven.wagon.http.ssl.insecure=true -Djavax.net.ssl.trustStore="/usr/lib/jvm/java-1.8.0-openjdk-1.8.0.242.b08-4.el8.x86_64/jre/lib/security/cacerts" -Djavax.net.ssl.trustStorePassword=changeit


